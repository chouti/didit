{
	"name": "Did it",
	"description": "Did it is a simple web application, you can use it to log what you've done during the day.",
	"version":"0.0.1",
	"app":{
		"launch":{
			"local_path":"didit.html"
		}
	},
	"icons":{
		"16":"img/icon_16.png",
		"32":"img/icon_32.png",
		"64":"img/icon_64.png",
		"128":"img/icon_128.png",
		"512":"img/icon_512.png"
	},
	"permissions":[
		"tabs",
		"https://*.doit.im/",
		"http://*.doit.im"
	]
}